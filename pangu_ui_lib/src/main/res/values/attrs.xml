    <declare-styleable name="PanguInputView">
        <attr name="pgiv_title_size" format="dimension" />
        <attr name="pgiv_title_text_style">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
